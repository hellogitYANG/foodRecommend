<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.foodrecommend.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.example.foodrecommend.beans.User">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="openId" column="open_id" jdbcType="VARCHAR"/>
            <result property="userName" column="user_name" jdbcType="VARCHAR"/>
            <result property="phone" column="phone" jdbcType="VARCHAR"/>
            <result property="foodStats" column="food_stats" jdbcType="VARCHAR"/>
            <result property="authority" column="authority" jdbcType="INTEGER"/>
            <result property="isBan" column="is_ban" jdbcType="INTEGER"/>
            <result property="collectFoodSku" column="collect_food_sku" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,open_id,user_name,
        phone,food_stats,authority,
        is_ban,collect_food_sku,create_time,
        update_time
    </sql>
</mapper>
